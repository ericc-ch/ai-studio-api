{
  "name": "ai-studio-api",
  "version": "0.0.0",
  "description": "Turn Google AI Studio into OpenAI API compatible server. The stupid way.",
  "homepage": "https://github.com/ericc-ch/ai-studio-api",
  "bugs": "https://github.com/ericc-ch/ai-studio-api/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericc-ch/ai-studio-api.git"
  },
  "author": "Erick Christian <erickchristian48@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "dev": "tsx watch ./src/main.ts",
    "knip": "knip",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish --access public",
    "prestart": "pnpm run build",
    "start": "node ./dist/main.js"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": "pnpm run lint --fix"
  },
  "dependencies": {
    "@google/genai": "^1.6.0",
    "citty": "^0.1.6",
    "clipboardy": "^4.0.0",
    "consola": "^3.4.2",
    "hono": "^4.8.2",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.1",
    "srvx": "^0.8.0",
    "tiny-invariant": "^1.3.3",
    "tinyexec": "^1.0.1",
    "tsx": "^4.20.3"
  },
  "devDependencies": {
    "@echristian/eslint-config": "^0.0.43",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.3",
    "bumpp": "^10.2.0",
    "eslint": "^9.29.0",
    "jiti": "^2.4.2",
    "knip": "^5.61.2",
    "lint-staged": "^16.1.2",
    "prettier-plugin-packagejson": "^2.5.15",
    "simple-git-hooks": "^2.13.0",
    "tsdown": "^0.12.8",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.12.1"
}
