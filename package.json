{
  "name": "ai-studio-api",
  "version": "0.0.0",
  "description": "Turn Google AI Studio into OpenAI API compatible server. The stupid way.",
  "homepage": "https://github.com/ericc-ch/ai-studio-api",
  "bugs": "https://github.com/ericc-ch/ai-studio-api/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericc-ch/ai-studio-api.git"
  },
  "author": "Erick Christian <erickchristian48@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "dev": "tsx watch ./src/main.ts",
    "knip": "knip-bun",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish --access public",
    "prestart": "bun run build",
    "start": "node ./dist/main.js"
  },
  "simple-git-hooks": {
    "pre-commit": "bunx lint-staged"
  },
  "lint-staged": {
    "*": "bun run lint --fix"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "hono": "^4.7.10",
    "jsdom": "^26.1.0",
    "playwright": "^1.52.0",
    "srvx": "^0.7.1",
    "tiny-invariant": "^1.3.3",
    "tinyexec": "^1.0.1",
    "tsx": "^4.19.4"
  },
  "devDependencies": {
    "@echristian/eslint-config": "^0.0.42",
    "@types/bun": "^1.2.13",
    "@types/jsdom": "^21.1.7",
    "bumpp": "^10.1.1",
    "eslint": "^9.27.0",
    "jiti": "^2.4.2",
    "knip": "^5.56.0",
    "lint-staged": "^16.0.0",
    "prettier-plugin-packagejson": "^2.5.13",
    "simple-git-hooks": "^2.13.0",
    "tsdown": "^0.11.11",
    "typescript": "^5.8.3"
  }
}
